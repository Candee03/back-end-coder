<table class="table">
    <thead>
    <tr>
        <th scope="col">Full Name</th>
        <th scope="col">Role</th>
        <th scope="col">Age</th>
        <th scope="col">Email</th>
        <th scope="col">Id</th>
        <th scope="col">Image</th>
        <th scope="col">options</th>
    </tr>
    </thead>
    <tbody>
    {{#each users}}
        <tr style="height: 5rem;">
            <td>{{this.first_name}} {{this.last_name}}</td>
            <td>{{this.role}}</td>
            <td>{{this.age}}</td>
            <td>{{this.email}}</td>
            <td>{{this._id}}</td>
            <td><img src="{{this.img}}" class="img-fluid" style="width: 4rem;" alt=""></td>
            <td>
                <button onclick='deleteUser("{{this._id}}", "{{this.cartId}}")'>Eliminar Usuario</button>
                <button disabled="true" onclick='changeToPremium("{{this._id}}")'>Cambiar a Premium</button>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>

<script src="/js/deleteUser.js"></script>